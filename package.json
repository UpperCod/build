{
    "name": "@devserver/build",
    "type": "module",
    "version": "0.3.0",
    "description": "build for @devserver/cli",
    "main": "./build.cjs",
    "module": "./src/build.js",
    "typings": "types.d.ts",
    "exports": {
        ".": {
            "import": "./src/build.js",
            "require": "./build.cjs"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "ava",
        "example": "node tests/build.js",
        "prepare": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UpperCod/devserver-build.git"
    },
    "author": "UpperCod",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/UpperCod/devserver-build/issues"
    },
    "homepage": "https://github.com/UpperCod/devserver-build#readme",
    "dependencies": {
        "@devserver/replace-import": "^0.3.1",
        "@uppercod/hash": "^1.1.1",
        "fast-glob": "^3.2.4",
        "rollup": "^2.21.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "devDependencies": {
        "@betit/rollup-plugin-rename-extensions": "^0.1.0",
        "atomico": "^1.9.1",
        "ava": "^3.10.1",
        "builtin-modules": "^3.1.0",
        "rollup": "^2.21.0"
    },
    "ava": {
        "files": [
            "tests/**/*.test.js"
        ]
    },
    "prettier": {
        "tabWidth": 4,
        "singleQuote": false
    }
}
